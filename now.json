{
    "version": 2,
    "name": "ginkgo-web",
    "alias": "ginkgo.now.sh",
    "regions": ["hnd1"],
    "builds": [
        {
            "src": "public/**/*",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/assets/.+",
            "headers": { "cache-control": "maxage=31557600" },
            "continue": true
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}
