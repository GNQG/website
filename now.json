{
    "version": 2,
    "name": "ginkgo-web",
    "alias": "ginkgo.now.sh",
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": { "distDir": "public" }
        }
    ],
    "routes": [
        {
            "src": "/assets/.+",
            "headers": { "cache-control": "maxage=31557600" }
        }
    ]
}
